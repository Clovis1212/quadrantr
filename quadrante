Algoritmo "quadrante"

Var
  X, Y : real


Inicio

 escreval("Digite os valores de X e Y(respectivamente): ")
 leia(X)
 leia(Y)

 enquanto (x <> 0) e (y <> 0 ) faca
 
    se (x > 0) e (y > 0) entao
          escreval("QUADRANTE 1")
    senao
       se (x < 0) e (y > 0) entao
             escreval ("QUADRANTE 2")
       senao

          se (x < 0) e (y < 0) entao
             escreval (" QUADRANTE 3")
          senao
             escreval (" QUADRANTE 4")
          fimse
       fimse
    fimse
    escreval("Digite os valores de X e Y(respectivamente): ")
    leia(X)
    leia(Y)
 fimenquanto

Fimalgoritmo
